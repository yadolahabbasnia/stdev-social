{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "NestJS Debug",
        "runtimeExecutable": "yarn",
        "runtimeArgs": ["run", "debug"],
        "port": 9229,
        "skipFiles": ["<node_internals>/**"],
        "restart": true,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  